{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(eat|courses|home|index)?",
      "headers": [{ "key": "Content-Type", "value": "text/html; charset=utf-8" }]
    },
    {
      "source": "/recipes.json",
      "headers": [{ "key": "Content-Type", "value": "application/json; charset=utf-8" }]
    },
    {
      "source": "/css/(.*)",
      "headers": [{ "key": "Content-Type", "value": "text/css; charset=utf-8" }]
    }
  ]
}
